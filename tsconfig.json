{
  "compilerOptions": {
    "target": "ES2020", // or higher
    "module": "ESNext",
    "lib": [
      "DOM",
      "ESNext"
    ],
    // "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "outDir": "./dist"
  },
  "include": [
    "src/**/*"
  ]
}